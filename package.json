{
  "name": "restaurante-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --proxy-config proxy.conf.json",
    "start:prod:local": "npx serve dist/restaurante-frontend/browser",
    "serve:ssr:restaurante-frontend": "node dist/restaurante-frontend/server/server.mjs",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest --coverage",
    "lint": "eslint src/**/*.{ts,js,html}",
    "format": "prettier \"src/**/*.{ts,html,scss,md}\" --write",
    "format:check": "prettier \"src/**/*.{ts,html,scss,md}\" --check",
    "images:generate": "node tools/generate-images.mjs",
    "images:png2webp": "node tools/png-to-webp.mjs",
    "postbuild": "node tools/generate-sitemap.mjs",
    "limpieza": "npx rimraf node_modules package-lock.json .angular/cache && npm install --prefer-online",
    "deps:unused": "npx depcheck",
    "updates": "npm-check-updates -u && npm install",
    "lint:scss": "stylelint src/**/*.scss --fix",
    "validate:mocks": "node tools/validate-mocks.mjs",
    "lint:fix": "eslint \"src/**/*.{ts,js}\" --fix",
    "lint:fix:imports": "eslint \"src/**/*.{ts,js}\" --fix --rule \"simple-import-sort/imports:error\" --rule \"simple-import-sort/exports:error\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.1.6",
    "@angular/common": "20.1.6",
    "@angular/compiler": "20.1.6",
    "@angular/core": "20.1.6",
    "@angular/forms": "20.1.6",
    "@angular/platform-browser": "20.1.6",
    "@angular/platform-browser-dynamic": "20.1.6",
    "@angular/platform-server": "^20.3.0",
    "@angular/router": "20.1.6",
    "@angular/service-worker": "20.1.6",
    "@angular/ssr": "^20.3.1",
    "@fortawesome/fontawesome-free": "^7.0.1",
    "bootstrap": "^5.3.3",
    "express": "^5.1.0",
    "jwt-decode": "^4.0.0",
    "ngx-toastr": "^19.0.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.1",
    "@angular/cli": "20.1.6",
    "@angular/compiler-cli": "20.1.6",
    "@testing-library/jest-dom": "^6.8.0",
    "@types/express": "^5.0.3",
    "@types/jest": "29.5.12",
    "@types/node": "^24.0.12",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "depcheck": "^1.4.7",
    "eslint": "*",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jest": "29.7.0",
    "jest-preset-angular": "14.6.1",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.6.2",
    "sharp": "^0.34.3",
    "stylelint": "^16.21.1",
    "ts-jest": "29.4.1"
  }
}
