{
  "name": "restaurante-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest --coverage",
    "lint": "eslint src/**/*.{ts,js,html}",
    "format": "prettier \"src/**/*.{ts,html,scss,md}\" --write",
    "format:check": "prettier \"src/**/*.{ts,html,scss,md}\" --check",
    "e2e": "ng e2e",
    "limpieza": "npx rimraf node_modules package-lock.json .angular/cache && npm install --prefer-online",
    "deps:unused": "npx depcheck",
    "updates": "npm-check-updates -u && npm install",
    "lint:scss": "stylelint src/**/*.scss --fix",
    "validate:mocks": "node tools/validate-mocks.mjs",
    "serve:ssr:restaurante-frontend": "node dist/restaurante-frontend/server/server.mjs",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint:fix": "eslint \"src/**/*.{ts,js}\" --fix",
    "lint:fix:imports": "eslint \"src/**/*.{ts,js}\" --fix --rule \"simple-import-sort/imports:error\" --rule \"simple-import-sort/exports:error\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.1.6",
    "@angular/common": "20.1.6",
    "@angular/compiler": "20.1.6",
    "@angular/core": "20.1.6",
    "@angular/forms": "20.1.6",
    "@angular/platform-browser": "20.1.6",
    "@angular/platform-browser-dynamic": "20.1.6",
    "@angular/platform-server": "20.1.6",
    "@angular/router": "20.1.6",
    "@angular/service-worker": "20.1.6",
    "@angular/ssr": "20.1.6",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.7",
    "depcheck": "^1.4.7",
    "express": "^5.1.0",
    "jwt-decode": "^4.0.0",
    "lightbox2": "^2.11.5",
    "ngx-toastr": "^19.0.0",
    "owl.carousel": "^2.3.4",
    "rxjs": "~7.8.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "20.1.6",
    "@angular-eslint/builder": "*",
    "@angular-eslint/eslint-plugin": "*",
    "@angular-eslint/eslint-plugin-template": "*",
    "@angular-eslint/schematics": "*",
    "@angular-eslint/template-parser": "*",
    "@angular/cli": "20.1.6",
    "@angular/compiler-cli": "20.1.6",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@cypress/schematic": "^4.1.1",
    "@netlify/angular-runtime": "^2.4.0",
    "@testing-library/angular": "^18.0.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@types/express": "^5.0.3",
    "@types/jest": "29.5.12",
    "@types/node": "^24.0.12",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "cypress": "latest",
    "eslint": "*",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jest": "29.7.0",
    "jest-preset-angular": "14.6.1",
    "prettier": "^3.6.2",
    "stylelint": "^16.21.1",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-order": "^7.0.0",
    "ts-jest": "29.4.1"
  }
}
